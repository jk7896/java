package part04_ClassTest;

public class Test_call {

	public static void main(String[] args) {
		Test ts=new Test();
		ts.tall=174;
		ts.weight=78;
		ts.name="김범수";
		ts.age=41;

		System.out.println("키가"+ts.tall+"몸무게가"+ts.weight+"인 남성이 있습니다. 이름은 "+ts.name+"이고 나이는 "+ts.age+"입니다.");
		
		ts.tall=180;
		ts.weight=75;
		ts.name="박효신";
		ts.age=42;

		System.out.println("키가"+ts.tall+"몸무게가"+ts.weight+"인 남성이 있습니다. 이름은 "+ts.name+"이고 나이는 "+ts.age+"입니다.");
	}

}
