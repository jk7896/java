package part03_ClassTest;

public class Test {
	int tall;
	int weight;
	String name;
	int age;

public Test(int _tall,int _weight,String _name,int _age) {
	this.tall=_tall;
	this.weight=_weight;
	this.name=_name;
	this.age=_age;
	System.out.println("키가"+tall+"몸무게가"+weight+"인 남성이 있습니다. 이름은 "+name+"이고 나이는 "+age+"입니다.");
	}
}